{
   "name": "new-typeorm-project",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "devDependencies": {
      "@types/jest": "^27.0.3",
      "@types/node": "^8.0.29",
      "jest": "^27.4.3",
      "nodemon": "^2.0.22",
      "supertest": "^6.1.6",
      "ts-jest": "^27.1.0",
      "ts-node": "3.3.0",
      "typescript": "^3.3.3333"
   },
   "dependencies": {
      "body-parser": "^1.18.1",
      "dotenv": "^10.0.0",
      "express": "^4.15.4",
      "express-validator": "^6.13.0",
      "morgan": "^1.10.0",
      "pg": "^8.4.0",
      "pre-commit": "^1.2.2",
      "reflect-metadata": "^0.1.10",
      "rimraff": "^0.0.1-security",
      "sqlite3": "^5.1.7",
      "typeorm": "0.2.40"
   },
   "scripts": {
      "start1": "ts-node src/index.ts",
      "start": "nodemon src/index.ts",
      "build": "rimraf dist && tsc",
      "ts.check": "tsc --project tsconfig.json",
      "add-build": "git add dist",
      "dev": "nodemon -w *.ts -w .env src/index.ts",
